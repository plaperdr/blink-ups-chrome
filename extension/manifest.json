{
  "manifest_version": 2,

  "name": "User-Profile Sync",
  "short_name": "UPS",
  "description": "Profile synchronisation extension for Blink",
  "version": "0.1",
  "author": "Blink",
  "offline_enabled": true,
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
	"nativeMessaging"
  ]
}

